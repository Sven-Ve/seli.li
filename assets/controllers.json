{
    "controllers": {
        "@svc/log-bundle": {
            "view": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@svc/util-bundle": {
            "clipboard": {
                "enabled": true,
                "fetch": "lazy"
            },
            "mclipboard": {
                "enabled": true,
                "fetch": "lazy"
            },
            "alert": {
                "enabled": true,
                "fetch": "lazy"
            },
            "submit-confirm": {
                "enabled": true,
                "fetch": "lazy"
            },
            "wysiwyg": {
                "enabled": true,
                "fetch": "lazy"
            },
            "show-password": {
                "enabled": true,
                "fetch": "lazy"
            },
            "reload-content": {
                "enabled": true,
                "fetch": "lazy"
            },
            "modal": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@symfony/ux-autocomplete": {
            "autocomplete": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": true,
                    "tom-select/dist/css/tom-select.bootstrap5.css": false
                }
            }
        },
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
