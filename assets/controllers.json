{
    "controllers": {
        "@svc/log-bundle": {
            "view": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@svc/util-bundle": {
            "clipboard": {
                "enabled": true,
                "fetch": "lazy"
            },
            "mclipboard": {
                "enabled": false,
                "fetch": "lazy"
            },
            "alert": {
                "enabled": true,
                "fetch": "lazy"
            },
            "submit-confirm": {
                "enabled": true,
                "fetch": "lazy"
            },
            "wysiwyg": {
                "enabled": false,
                "fetch": "lazy"
            },
            "show-password": {
                "enabled": false,
                "fetch": "lazy"
            },
            "reload-content": {
                "enabled": false,
                "fetch": "lazy"
            },
            "modal": {
                "enabled": true,
                "fetch": "lazy"
            },
            "toast": {
                "enabled": true,
                "fetch": "lazy"
            },
            "autosubmit": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@symfony/ux-autocomplete": {
            "autocomplete": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "tom-select/dist/css/tom-select.default.css": false,
                    "tom-select/dist/css/tom-select.bootstrap4.css": false,
                    "tom-select/dist/css/tom-select.bootstrap5.css": true
                }
            }
        },
        "@symfony/ux-toggle-password": {
            "toggle-password": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@symfony/ux-toggle-password/dist/style.min.css": true
                }
            }
        },
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
